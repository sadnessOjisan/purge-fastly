    "lint:fix": "yarn run _eslint -- --fix",
    "_prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,json,yaml,yml}'",
    "format:fix": "npm run _prettier -- --write",
    "format:check": "npm run _prettier -- --check",
    "test": "echo ok"
